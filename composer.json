{
    "name": "plumtreesystems/file-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "knplabs/gaufrette": "^0.8.1",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/orm-pack": "^1.0",
        "symfony/security": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/yaml": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PlumTreeSystems\\FileBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PlumTreeSystems\\FileBundle\\Tests\\": "Tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/test-pack": "^1.0"
    }
}
