{
    "name": "plumtreesystems/file-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "gaufrette/aws-s3-adapter": "^0.4.0",
        "google/apiclient": "^2.0",
        "knplabs/gaufrette": "^0.9",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.6",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PlumTreeSystems\\FileBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PlumTreeSystems\\FileBundle\\Tests\\": "Tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/test-pack": "^1.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
